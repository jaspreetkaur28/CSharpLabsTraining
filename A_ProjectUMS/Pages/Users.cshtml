@page
@model A_ProjectUMS.Pages.UsersModel


<h1>Users</h1>
<hr />

<table class="table table-striped table-hover table-bordered">
    <tr>
        <th>Name</th>
    </tr>
    @foreach (var item in Model.UsersList)
    {
        <tr>
            <td>
                <a asp-page="./EditUsers" asp-route-id="@item.UsersID">@(item.FirstName + " " + item.LastName)</a>
            </td>
        </tr>
    }

</table>
